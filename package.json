{
  "name": "loggerboggers-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build",
    "start": "next start",
    "start:railway": "next start -p ${PORT:-3000}",
    "lint": "next lint",
    "dedalus": "node --env-file=${DEDALUS_ENV_FILE:-.env} scripts/dedalus.mjs",
    "dedalus:chat": "node --env-file=${DEDALUS_ENV_FILE:-.env} scripts/dedalus-chat.mjs",
    "dedalus:ocr": "node --env-file=${DEDALUS_ENV_FILE:-.env} scripts/dedalus-ocr.mjs",
    "web": "npm run dev --workspace=@repo/web"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.95.2",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "clsx": "^2.1.1",
    "framer-motion": "^12.33.0",
    "lucide-react": "^0.542.0",
    "next": "16.1.6",
    "openai": "^4.104.0",
    "plaid": "^31.0.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-plaid-link": "^4.1.1",
    "reactflow": "^11.11.4",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.4.0",
    "yahoo-finance2": "^3.2.4",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^20.19.11",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.0",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.2"
  }
}
